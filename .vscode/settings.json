{
	"workbench.colorTheme": "Ayu Mirage Bordered",
	"files.trimTrailingWhitespace": true,
	"editor.tabSize": 2,
	"window.newWindowDimensions": "fullscreen",
	"notebook.cellToolbarLocation": {
		"default": "right",
		"jupyter-notebook": "left"
	},
	"diffEditor.wordWrap": "on",
	"editor.wordWrapColumn": 1000,
	"editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
	"bracketPairColorizer.depreciation-notice": false,
	"security.workspace.trust.untrustedFiles": "open",
	"git.confirmSync": false,
	"genieai.azure.url": "",
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"editor.formatOnPaste": false,
	"editor.formatOnType": false,
	"files.autoSave": "afterDelay",
	"editor.fontFamily": "'Ricty Diminished', Menlo, Monaco, 'Courier New', monospace",
	"update.mode": "none",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"editor.minimap.renderCharacters": false,
	"editor.minimap.showSlider": "always",
	"emmet.excludeLanguages": [],
	"emmet.includeLanguages": {
		"markdown": "html"
	},
	"emmet.variables": {
		"lang": "ja",
		"charset": "UTF-8"
	},
	"auto-close-tag.activationOnLanguage": [
		"markdown",
		"xml",
		"php",
		"blade",
		"ejs",
		"jinja",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"plaintext",
		"markdown",
		"vue",
		"liquid",
		"erb",
		"lang-cfml",
		"cfml",
		"HTML (EEx)",
		"HTML (Eex)",
		"plist"
	],
	"auto-close-tag.disableOnLanguage": [],
	"auto-close-tag.excludedTags": ["area", "base", "br", "col", "command", "embed", "hr", "img", "input", "keygen", "link", "meta", "param", "source", "track", "wbr"],
	"auto-rename-tag.activationOnLanguage": ["*"],
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"prettier.documentSelectors": ["**/*.astro"],
	"[astro]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"astro.content-intellisense": true,
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"cssVariables.lookupFiles": ["**/*.ts", "**/*.astro", "**/*.css", "**/*.scss", "**/*.sass", "**/*.less"],
	"cssVariables.languages": [
		"astro",
		"svelte",
		"vue",
		"vue-html",
		"vue-postcss",
		"scss",
		"postcss",
		"less",
		"css",
		"html",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"source.css.styled"
	],
	"px-to-vw.viewportWidth": 375,
	"path-intellisense.autoSlashAfterDirectory": false,
	"path-intellisense.autoTriggerNextSuggestion": false,
	"path-intellisense.ignoreTsConfigBaseUrl": false,
	"path-intellisense.mappings": {},
	"disable-hardware-acceleration": true,
	"workbench.sideBar.location": "right",
	"playwright.reuseBrowser": true,
	"git.suggestSmartCommit": false,
	"codeium.enableConfig": {
		"*": true
	},
	"geminicodeassist.project": "",
	"diffEditor.codeLens": true,
	"window.zoomLevel": 1,
	"prettier.printWidth": 150,
	"workbench.iconTheme": "material-icon-theme",
	"extensions.autoUpdate": "onlyEnabledExtensions",
	"cSpell.userWords": ["admaxads"],
	"typescript.tsdk": "node_modules/typescript/lib"
}
