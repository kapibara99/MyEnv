{
	"extends": ["../../tsconfig.json"],
	"include": ["src", "./"],
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"@/*": ["./src/_modules/*"],
			"~/*": ["./public/*"],
			"#/*": ["./src/*"]
		},
		"types": ["vite/client"],
		/* Bundler mode */
		"moduleResolution": "bundler",
		"skipLibCheck": true // *.d.ts „ÅÆskip
	}
}
