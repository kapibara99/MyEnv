{
	"name": "ui-catalog-used-astro",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"watch": "run-p watch:*",
		"format": "run-p format:*",
		"preview": "astro preview",
		"build": "astro check && tsc --noEmit && astro build && npm run afterBuild",
		"afterBuild": "node scripts/newOrderCSSFiles.js && node scripts/removeCommentsInCSSFiles.js",
		"format:eslint": "eslint --ext \"./**/*.ts\" \"./**/*.astro\" --fix",
		"format:prettier": "prettier --write src/**/*.{astro,ts,tsx,scss} --config ./.prettierrc.json",
		"format:html": "markuplint \"src/**/*.{html,astro}\" --fix",
		"watch:eslint": "onchange --initial -d 500 \"src/**/*.{ts,tsx}\" -- eslint \"src/**/*.{ts,tsx}\" && echo Success!",
		"watch:scss": "hcm src/**/*.module.scss --watch",
		"watch:html": "onchange --initial -d 500 \"src/**/*.html\" -- npm run lint:html",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@astrojs/check": "^0.5.6",
		"@astrojs/react": "^3.0.10",
		"astro": "^4.4.9",
		"astro-compress": "^2.3.1",
		"astro-purgecss": "^4.6.0",
		"purgecss": "^6.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"typescript": "^5.4.5"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^1.2.25",
		"@markuplint/astro-parser": "^4.6.7",
		"@storybook/addon-essentials": "^8.0.5",
		"@storybook/addon-interactions": "^8.0.5",
		"@storybook/addon-links": "^8.0.5",
		"@storybook/addon-onboarding": "^8.0.5",
		"@storybook/blocks": "^8.0.5",
		"@storybook/react": "^8.0.5",
		"@storybook/react-vite": "^8.0.5",
		"@storybook/test": "^8.0.5",
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.4.0",
		"@typescript-eslint/parser": "^7.4.0",
		"autoprefixer": "^10.4.20",
		"cheerio": "^1.0.0",
		"cssnano": "^7.0.6",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-astro": "^0.33.1",
		"eslint-plugin-storybook": "^0.8.0",
		"happy-css-modules": "^3.1.1",
		"markuplint": "^4.9.4",
		"npm-run-all": "^4.1.5",
		"onchange": "^7.1.0",
		"postcss-discard-comments": "^7.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-astro": "^0.12.2",
		"sass": "^1.75.0",
		"satori": "^0.10.13",
		"sharp": "^0.33.3",
		"storybook": "^8.0.5"
	},
	"browserslist": [
		"last 3 versions , not dead",
		"not IE >= 0",
		"iOS >= 12, Safari >= 14"
	],
	"volta": {
		"node": "18.17.1",
		"npm": "9.6.7"
	}
}
